@using System.Activities.Statements
@using SwcLmsMastery.Models.DBModels
@model SwcLmsMastery.Models.ApplicationUser

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@{
    if ((User.IsInRole("Administrator")))
    {
        Html.RenderPartial("_AdminDash");
    }
    else if ((User.IsInRole("Teacher")))
    {
        Html.RenderPartial("_TeacherDash");
    }
    else if ((User.IsInRole("Student")))
    {
        Html.RenderPartial("_StudentDash");
    }
    else if ((User.IsInRole("Parent")))
    {
        Html.RenderPartial("_ParentDash");
    }
    else
    {
        Html.RenderPartial("_AwaitingAuth");
    }

}

