@using System.Collections
@using SwcLmsMastery.Models
@model SwcLmsMastery.Models.DBModels.LmsUser


@{
    ViewBag.Title = "Parent Dashboard";

}

<div class="col-lg-12">
    <div align="center">
        <h3>@ViewBag.Title</h3>


        @{ using (Html.BeginForm("LogOff", "Account", FormMethod.Post, htmlAttributes: new { id = "logoutForm", title = "LogOff" }))
         {
            @Html.AntiForgeryToken()

            <a href="javascript:document.getElementById('logoutForm').submit()"><button type="button" class="btn btn-primary">Log Off</button></a>

         }
        }




    </div>
</div>